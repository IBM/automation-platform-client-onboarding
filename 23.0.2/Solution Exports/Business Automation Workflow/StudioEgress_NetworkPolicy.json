{
	"apiVersion": "networking.k8s.io/v1",
	"kind": "NetworkPolicy",
	"metadata": {
		"name": "deploytool-studio-egress-allow-all",
		"namespace": "$$(cp4baNamespace)$$"
	},
	"spec": {
		"podSelector": {
			"matchLabels": {
				"app.kubernetes.io/instance": "$$(cp4baClusterName)$$",
				"statefulset.kubernetes.io/pod-name=icp4adeploy": "$$(cp4baClusterName)$$-bastudio-deployment-0"
			}
		},
		"policyTypes": [
			"Egress"
		],
		"egress": [
			{}
		]
	}
}